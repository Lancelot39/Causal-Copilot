{
    "directlingam": {
        "terms": [
            {"name": "np3m2_term", "expression": "N * p**3 * M**2"},
            {"name": "p4m3_term", "expression": "p**4 * M**3"}
        ],
        "min_runtime": 10,
        "log_transform": false,
        "param_calculations": {
            "M": {"type": "rank", "expression": "max(int(log(min(N, p))/log(2)), 2)"}
        }
    },
    "acceleratedlingam": {
        "terms": [
            {"name": "np3m2_term", "expression": "N * p**3 * M**2"},
            {"name": "p4m3_term", "expression": "p**4 * M**3"}
        ],
        "min_runtime": 10,
        "log_transform": false,
        "param_calculations": {
            "M": {"type": "rank", "expression": "max(int(log(min(N, p))/log(2)), 2)"}
        }
    },
    "pc": {
        "terms": [
            {"name": "log_p", "expression": "log(p)"},
            {"name": "log_d", "expression": "log(d)"},
            {"name": "log_exp_d", "expression": "d * log(2)"},
            {"name": "log_N", "expression": "log(N)"}
        ],
        "min_runtime": 10,
        "log_transform": true,
        "param_calculations": {
            "d": {"type": "degree", "expression": "int(density * (p - 1))"}
        }
    },
    "ges": {
        "terms": [
            {"name": "log_p2", "expression": "log(p * p)"},
            {"name": "log_exp_k", "expression": "k * log(2)"},
