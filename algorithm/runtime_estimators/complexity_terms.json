            {"name": "log_iter", "expression": "log(E)"}
        ],
        "min_runtime": 10,
        "log_transform": true,
        "param_calculations": {
            "E": {"type": "edges", "expression": "int((p * (p - 1)) / 2 * density)"},
            "k": {"type": "derived", "expression": "int(log(E)/log(2)) if E > 0 else 0"}
        }
    },
